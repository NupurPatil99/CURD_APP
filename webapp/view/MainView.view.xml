<mvc:View controllerName="com.sap.btp.curdapp.curdapp.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
    <content>
    <Table id="_IDGenTable" items="{/Categories}" >
      <columns>
        <Column id="_IDGenColumn">
          <Label id="_IDGenLabel" text="ID" />
        </Column>
        <Column id="_IDGenColumn1">
          <Text id="_IDGenText" text="Name" />
        </Column>
      </columns>
      <items>
        <ColumnListItem id="_IDGenColumnListItem1">
          <cells>
            <Text id="_IDGenText1" text="{ID}" />
            <Text id="_IDGenText2" text="{Name}" />
          </cells>
        </ColumnListItem>
      </items>

    </Table>
    </content>
    </Page>
</mvc:View>